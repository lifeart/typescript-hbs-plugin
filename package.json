{
  "name": "typescript-hbs-plugin",
  "version": "1.0.0",
  "description": "TypeScript language service plugin that adds IntelliSense for hbs tagged templates",
  "keywords": [
    "TypeScript",
    "hbs",
    "glimmer",
    "handlebars",
    "literal hbs",
    "templates"
  ],
  "main": "lib/index.js",
  "author": "Aleksandr Kanunnikov <lifeart92@mgail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeart/typescript-hbs-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/lifeart/typescript-hbs-plugin/issues"
  },
  "dependencies": {
    "typescript-styled-plugin": "^0.18.1",
    "typescript-template-language-service-decorator": "^2.2.0",
    "vscode-html-languageservice": "4.1.0",
    "vscode-languageserver-textdocument": "^1.0.2",
    "vscode-languageserver-types": "3.16.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/node": "^12.0.0",
    "chai": "^4.1.2",
    "glob": "^7.1.3",
    "mocha": "^8.3.0",
    "tslint": "^6.1.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "compile": "./node_modules/.bin/tsc -p .",
    "watch:compile": "./node_modules/.bin/tsc --watch -p .",
    "e2e": "./node_modules/.bin/mocha e2e/tests --slow 2000 --timeout 10000",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json"
  }
}
